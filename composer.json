{
    "name": "ryssbowh/craft-triggers",
    "description": "Run actions when events are triggered in Craft",
    "type": "craft-plugin",
    "license": "MIT",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "trigger",
        "event",
        "craft-plugin"
    ],
    "authors": [
        {
            "name": "The Web Puzzlers",
            "homepage": "https://github.com/ryssbowh"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
          "Ryssbowh\\CraftTriggers\\": "src/"
        }
    },
    "extra": {
        "name": "Triggers",
        "handle": "triggers",
        "developer": "The Web Puzzlers",
        "developerUrl": "https://www.webpuzzlers.co.uk",
        "documentationUrl": "https://www.webpuzzlers.co.uk/plugins/triggers/2.x",
        "class": "Ryssbowh\\CraftTriggers\\Triggers"
    }
}
